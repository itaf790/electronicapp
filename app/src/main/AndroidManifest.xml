<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.electronicapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/phd"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Electronicapp">
        <activity android:name=".confirmuser"></activity>
        <activity
            android:name=".DetailsActivity"
            android:label=" Scheduled appointments"
            android:parentActivityName=".viewactivity" />
        <activity
            android:name=".about"
            android:label="About" />
        <activity
            android:name=".home"
            android:label="lOGIN AND REGISTRATION" />
        <activity
            android:name=".viewactivity"
            android:label="View Appointment"
            android:parentActivityName=".AdminLoginActivity" />
        <activity
            android:name=".AdminLoginActivity"
            android:label="Login Activity" />
        <activity
            android:name=".MainActivity"
            android:label="Insert data" />
        <activity
            android:name=".LoginActivity"
            android:label="Login " />
        <activity
            android:name=".Register"
            android:label="Register"
            android:parentActivityName=".LoginActivity" />
        <activity
            android:name=".Splash"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>